<pdf>
    <dynamic-page>
      <h2>Informe de Baja de artículos</h2>
      <h5>Detalle de la Baja</h5>
      <h4>Realizada en: {{oficinaOrigen}} </h4>
      <h4>Responsable actual de oficina: {{oficinaOrigen.responsableOficina}}</h4>
      <h4>Fecha: {{baja.fecha|date('d-m-Y')}}</h4>
      <h4>Número de expediente: {{baja.expediente}}</h4>
      <h5>Listado de artículos dados de baja:</h5>
      {% for i in articulos %}
        <h4>Número de Inventario: {{i.numInventario}} - Denominación: {{i.denominacion}}</h4>
      {% endfor %}
      {% if tieneObservaciones %}
        <h5>Observaciones de la baja:</h5>
        <h4>{{baja.observaciones}}</h4>
      {% endif %}

  </dynamic-page>
</pdf>
